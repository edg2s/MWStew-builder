{
    "@metadata": {
{% if author|length > 0 %}
        "authors": [
            "{{ author }}"
        ]
{% else %}
        "authors": []
{% endif %}
    },
    "{{ lowerCamelName }}": "{{ title }}",
{% if not specialpage.exists %}
    "{{ lowerCamelName }}-desc": "{{ desc }}"
{% else %}
    "{{ lowerCamelName }}-desc": "{{ desc }}",
    "{{ specialpage.name.i18n }}-title": "{{ specialpage.title }}",
    "{{ specialpage.name.i18n }}-intro": "{{ specialpage.intro }}"
{% endif %}
}
